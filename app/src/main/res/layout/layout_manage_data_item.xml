<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_manage_data_relative"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="15dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/shape_item_white"
    android:padding="15dp">

    <TextView
        android:id="@+id/manage_data_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="郑州·中国中铁"
        android:textColor="@color/color_17191C"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_centerVertical="true"/>

    <ImageView
        android:id="@+id/manage_data_expand_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:src="@mipmap/icon_expand_no"
        android:layout_centerVertical="true"/>

</RelativeLayout>