                <TextView
                    android:id="@+id/title10"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="项目经理"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title9" />

                <TextView
                    android:id="@+id/resume_detail_use_boss_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title10"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_serial_tv" />

                <TextView
                    android:id="@+id/title11"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="进场时间"
                    android:textColor="@color/color_999999"
                    android:textSize="10sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title10" />

                <TextView
                    android:id="@+id/resume_detail_use_package_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="10sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title11"
