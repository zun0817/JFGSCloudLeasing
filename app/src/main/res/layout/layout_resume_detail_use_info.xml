            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/title8"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:drawablePadding="3dp"
                    android:text="设备类型"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/resume_detail_use_type_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title8"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/titleb"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="区间里程(m)"
                    android:textColor="@color/color_999999"
                    android:textSize="10sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title8" />

                <TextView
                    android:id="@+id/resume_detail_use_interval_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="10sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/titleb"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_type_tv" />

                <TextView
                    android:id="@+id/titlec"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="已验里程(m)"
                    android:textColor="@color/color_999999"
                    android:textSize="10sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/titleb" />

                <TextView
                    android:id="@+id/resume_detail_use_tested_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="10sp"
                    android:layout_marginTop="10dp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/titlec"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_interval_tv" />

                <TextView
                    android:id="@+id/title9"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="立项编号"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/titlec" />

                <TextView
                    android:id="@+id/resume_detail_use_serial_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title9"
