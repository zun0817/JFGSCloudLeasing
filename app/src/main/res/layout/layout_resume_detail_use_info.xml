<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/resume_detail_use_cl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_item_white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_item_white"
        android:orientation="vertical"
        android:paddingHorizontal="12dp"
        android:paddingVertical="15dp"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/resume_detail_use_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="中铁装备"
            android:textColor="@color/color_010F1D"
            android:textSize="17sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@drawable/shape_view_f8fefb"
            android:orientation="horizontal"
            android:paddingHorizontal="7dp"
            android:paddingVertical="12dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/title1"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:drawablePadding="3dp"
                    android:text="设备状态"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/resume_detail_use_status_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title1"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/titlea"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="环宽(mm)"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title1" />

                <TextView
                    android:id="@+id/resume_detail_use_ringwidth_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    android:layout_marginTop="10dp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/titlea"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_status_tv" />

                <TextView
                    android:id="@+id/title2"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="开挖直径(mm)"
                    android:textColor="@color/color_999999"
                    android:textSize="10sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/titlea" />

                <TextView
                    android:id="@+id/resume_detail_use_excavate_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="10sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title2"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_ringwidth_tv" />

                <TextView
