                <TextView
                    android:id="@+id/title4"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="使用城市"
                    android:textColor="@color/color_999999"
                    android:textSize="12sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title3" />

                <TextView
                    android:id="@+id/resume_detail_use_usecity_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="12sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title4"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_class_tv" />

                <TextView
                    android:id="@+id/title5"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:drawablePadding="3dp"
                    android:text="设备使用方"
                    android:textColor="@color/color_999999"
                    android:textSize="10sp"
                    app:drawableStartCompat="@mipmap/icon_circle_label"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/title4" />

                <TextView
                    android:id="@+id/resume_detail_use_use_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="无"
                    android:textColor="@color/color_262626"
                    android:textSize="10sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintLeft_toRightOf="@id/title5"
                    app:layout_constraintTop_toBottomOf="@id/resume_detail_use_usecity_tv" />

                <TextView
                    android:id="@+id/title6"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
